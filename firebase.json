{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/*"
    ],
    "rewrites": [
      {
        "source": "/benchmark1",
        "destination": "/benchmark1.html"
      },
      {
        "source": "*",
        "destination": "/index.html"
      }
    ]
  }
}